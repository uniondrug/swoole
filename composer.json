{
  "name": "uniondrug/swoole",
  "description": "Uniondrug swoole (based on fastd/swoole)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "janhuang",
      "email": "bboyjanhuang@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "symfony/console": "^3.0",
    "uniondrug/packet": "^2.0",
    "uniondrug/http": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Uniondrug\\Swoole\\": "src"
    },
    "files": [
      "helpers.php"
    ]
  },
  "suggests": {
    "ext-swoole": "Allow php swoole extension.",
    "ext-inotify": "Allow php inotify extension."
  }
}
